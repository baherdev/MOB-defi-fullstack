# Production Environment Variables
# Copy this file to .env and fill in your values

# MySQL
MYSQL_ROOT_PASSWORD=your-strong-root-password-here
MYSQL_USER=mob_user
MYSQL_PASSWORD=your-strong-password-here

# Symfony
APP_SECRET=your-32-char-secret-key-here-change-this

# JWT (if enabled)
JWT_SECRET_KEY=%kernel.project_dir%/config/jwt/private.pem
JWT_PUBLIC_KEY=%kernel.project_dir%/config/jwt/public.pem
JWT_PASSPHRASE=your-jwt-passphrase-here

# Traefik / Let's Encrypt
# Replace with your actual email for SSL certificates
LETSENCRYPT_EMAIL=votre-email@example.com

# Domain names
# Replace with your actual domains
DOMAIN=votre-domaine.com
API_DOMAIN=api.votre-domaine.com
